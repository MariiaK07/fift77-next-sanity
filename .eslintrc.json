{
  "extends": [
    "next/core-web-vitals",
    "airbnb"
  ],
  "rules": {
    "react/function-component-definition": 0,
    "react/react-in-jsx-scope": 0,
    "react/prop-types": 0,
    "import/no-unresolved": 0,
    "import/prefer-default-export": 0,
    "import/extensions": 0,
    "camelcase": 0,
    "arrow-parens": 0,
    "jsx-a11y/no-static-element-interactions": 0
  }
}
